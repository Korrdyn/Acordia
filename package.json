{
  "name": "acordia",
  "version": "0.0.0",
  "description": "A Discord client",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "format": "prettier . --write",
    "build": "tsup src --format cjs,esm --dts --clean --no-splitting"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korrdyn/Acordia.git"
  },
  "keywords": [
    "discord",
    "typescript"
  ],
  "author": "Korrdyn",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Korrdyn/Acordia/issues"
  },
  "homepage": "https://github.com/Korrdyn/Acordia#readme",
  "dependencies": {
    "@discordjs/collection": "^2.0.0",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/ws": "^1.0.2",
    "discord-api-types": "^0.37.76",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  }
}
